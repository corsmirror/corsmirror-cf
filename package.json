{
  "name": "corsmirror-cf",
  "version": "1.0.3",
  "description": "CORSmirror Cloudflare",
  "author": "Mark <mark@remarkablemark.org>",
  "scripts": {
    "build": "scripts/build.sh",
    "clean": "rm -rf coverage dist",
    "cypress:open": "cypress open",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "lint:tsc": "tsc --noEmit -p functions",
    "postinstall": "husky install",
    "start": "npm run build && wrangler pages dev dist"
  },
  "dependencies": {
    "costatus": "1.0.0"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "18.0.2",
    "@bahmutov/cypress-esbuild-preprocessor": "2.2.0",
    "@cloudflare/workers-types": "4.20230724.0",
    "@commitlint/cli": "17.6.7",
    "@commitlint/config-conventional": "17.6.7",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.2.0",
    "cypress": "12.17.2",
    "cypress-cucumber-steps": "2.10.0",
    "eslint": "8.46.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-tsdoc": "0.2.17",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "marked": "5.1.2",
    "typescript": "5.1.6",
    "wrangler": "3.4.0"
  },
  "private": true,
  "license": "MIT"
}
